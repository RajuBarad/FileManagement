    "@ckeditor/ckeditor5-build-classic": "^41.4.2",
    "@ckeditor/ckeditor5-build-decoupled-document": "^41.4.2",
    "@types/file-saver": "^2.0.7",
    "docx-preview": "^0.3.7",
